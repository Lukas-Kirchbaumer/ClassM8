<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.laubi.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cm8logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".NewAccountActivity" />
        <activity android:name=".UserSettingsActivity" />
        <activity android:name=".VoteActivity" />
        <activity android:name=".ClassSettingsActivity" />
        <activity android:name=".NewClassActivity" />
        <activity android:name=".FileShareActivity" />
        <activity android:name=".AddM8Activity" />
    </application>

</manifest>